<h2>My Listings</h2>
<a href="/listings/new" class="create-listing-btn">Create New Listing</a>

<% if (listings && listings.length > 0) { %>
  <!-- Reuse the browse-listings partial but pass a flag to indicate it's "My Listings" -->
  <%- include('../partials/browse-listings', { listings: listings, fromSearch: false, source: 'myListings', isMyListings: true }) %>
  <% } else { %>
  <p>You haven't created any listings yet.</p>
<% } %>
